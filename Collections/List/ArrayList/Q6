package arrayList;

import java.util.ArrayList;
import java.util.List;

public class Q6 {

	public static void main(String[] args) {
		
		//Write a Java program to remove the third element from an array list.

			List<String> a = new ArrayList<>();
			a.add("moni");
			a.add("sun");
			a.add("hello");
			a.add("moon");
			a.add("juiko");
			System.out.println(a);
			a.remove(3);
			System.out.println(a);
			
	}

}
