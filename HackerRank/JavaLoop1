import java.util.Scanner;

public class JavaLoop1 {

	public static void main(String[] args) {
		Scanner s = new Scanner(System.in);		 
		System.out.println("enter number");
	      int N =s.nextInt();
	      //System.out.println(N);
	      for(int i =1; i <=10; i++) {
	    	  int x = N*i;
	    	  System.out.println(N+" * "+ i +" = "+x);
	      }
		
	}
}
